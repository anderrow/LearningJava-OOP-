
public class Main {

	public static void main(String[] args) {
		int width=5;
		int height=7;
		
		for (int row=1;row<=height;row++) {
			for (int column=1;column<=width;column++) {
				if(column==1) System.out.print("*");
				if(row==7) System.out.print("*");
			}
			System.out.println();
		}
	}
}
